#   Set Paths
#   ------------------------------------------------------------

export PATH=$MAMP_PHP:$PATH
export NODE_PATH=/usr/local/lib/node_modules

export GOPATH=$HOME/go

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/steven/google-cloud-sdk/path.bash.inc' ]; then . '/Users/steven/google-cloud-sdk/path.bash.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/steven/google-cloud-sdk/completion.bash.inc' ]; then . '/Users/steven/google-cloud-sdk/completion.bash.inc'; fi
